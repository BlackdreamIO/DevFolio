    "hide-files.files": [
        ".vscode",
        ".turbo",
        "node_modules",
        "packages",
        ".npmrc",
        "apps/server/.turbo",
        "apps/server/dist",
        "apps/server/node_modules",
        "apps/web/.next",
        "apps/web/.turbo",
        "apps/web/fonts",
        "apps/web/app/favicon.ico",
        "apps/web/.lintstagedrc.js",
        "apps/web/commitlint.config.js",
        "apps/web/eslint.config.js",
        "apps/web/next-env.d.ts",
        "apps/web/next.config.js",
        "apps/web/README.md",
        "apps/web/node_modules"
